{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "palette": {
      "color1": "#FFD800",
      "color2": "#7902AA",
      "color2-light": "#9D03DC",
      "color3": "#FFF"
    },
    "blocks": [
      {
        "alignment": "left",
        "segments": [
          {
            "background": "p:color1",
            "foreground": "#000",
            "leading_diamond": "ÓÇ∂",
            "style": "diamond",
            "template": " Û∞å¢ {{ .HostName }} ",
            "type": "session"
          },
          {
            "background": "p:color2",
            "foreground": "#fff",
            "powerline_symbol": "ÓÇ∞",
            "style": "powerline",
            "template": " ÔäΩ {{ .UserName }} ",
            "type": "session"
          },
          {
            "background": "#303030",
            "foreground": "#fff",
            "foreground_templates": [
              "{{ if or (.Working.Changed) (.Staging.Changed) }}#ff9248{{ end }}",
              "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#f26d50{{ end }}",
              "{{ if gt .Ahead 0 }}#89d1dc{{ end }}",
              "{{ if gt .Behind 0 }}#f17c37{{ end }}"
            ],
            "powerline_symbol": "ÓÇ∞",
            "properties": {
              "fetch_stash_count": true,
              "fetch_status": true,
              "fetch_upstream_icon": true
            },
            "style": "powerline",
            "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
            "type": "git"
          },
          {
            "background": "p:color3",
            "foreground": "#000",
            "style": "diamond",
            "template": " ÓôÅ </>{{ .CurrentDate | date .Format }}\u2800",
            "trailing_diamond": "ÓÇ¥",
            "type": "time"
          }
        ],
        "type": "prompt"
      },
      {
        "alignment": "left",
        "newline": true,
        "segments": [
          {
            "foreground": "p:color1",
            "style": "plain",
            "template": " {{ if .WSL }}WSL at {{ end }}{{ .Icon }}  ÓäÖ ",
            "type": "os"
          },
          {
            "foreground": "p:color2-light",
            "style": "plain",
            "template": "{{ .Path }} ÓäÖ ",
            "type": "path"
          },
          {
            "foreground": "p:color3",
            "properties": {
              "mapped_shell_names": {
                "pwsh": "ÓØá ",
                "cmd": "ÓØÑ ",
                "bash": "ÓØä "
              }
            },
            "template": "{{ .Name }}",
            "style": "plain",
            "type": "shell"
          },
          {
            "foreground": "#fff",
            "style": "powerline",
            "template": "üíÖ",
            "type": "root"
          },
          {
            "foreground": "p:color3",
            "style": "plain",
            "template": " ÓäÖ",
            "type": "text"
          }
        ],
        "type": "prompt"
      }
    ],
    "console_title_template": "{{ .UserName }}@{{ .HostName }} in {{ .PWD }}",
    "final_space": true,
    "version": 2
  }
