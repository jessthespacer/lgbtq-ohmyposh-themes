    "palette": {
      "color1": "#B57EDC",
      "color2": "#FFF",
      "color3": "#4A8123",
      "color3-light": "#6DBD33"
    },
    "blocks": [
      {
        "alignment": "left",
        "segments": [
          {
            "background": "p:color1",
